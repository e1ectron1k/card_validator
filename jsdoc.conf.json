{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
      "include": ["src/js"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
      "destination": "./doc",
      "recurse": true,
      "readme": "./README.md"
  }
}